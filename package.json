    "raw-loader": "^2.0.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
